# Supabase project configuration
SUPABASE_URL=https://your-project-ref.supabase.co
SUPABASE_ANON_KEY=stub
SUPABASE_JWT_SECRET=stub

# OAuth configuration
TOKEN_AI_MCP_OAUTH=true
TOKEN_AI_MCP_PUBLIC_URL=https://dexter.cash/mcp
TOKEN_AI_MCP_PORT=3930
TOKEN_AI_MCP_TOKEN=
TOKEN_AI_MCP_CORS=*
# Optional: restrict loaded toolsets (comma-separated keys; leave blank for all)
TOKEN_AI_MCP_TOOLSETS=
TOKEN_AI_ENABLE_APPS_SDK=true

# Dexter API base (used by wallet resolver)
DEXTER_API_BASE_URL=http://127.0.0.1:3030
# Optional: shared demo wallet public key for guest sessions
TOKEN_AI_DEFAULT_WALLET_ADDRESS=

# OIDC endpoints (defaults assume API is serving /api/connector/oauth/*)
TOKEN_AI_OIDC_ISSUER=https://dexter.cash
TOKEN_AI_OIDC_AUTHORIZATION_ENDPOINT=https://dexter.cash/api/connector/oauth/authorize
TOKEN_AI_OIDC_TOKEN_ENDPOINT=https://dexter.cash/api/connector/oauth/token
TOKEN_AI_OIDC_USERINFO=https://dexter.cash/api/connector/oauth/userinfo
TOKEN_AI_OIDC_SCOPES=openid profile email wallet.read wallet.trade
TOKEN_AI_OIDC_CLIENT_ID=cid_example

# Pumpstream harness configuration (scripts/run-pumpstream-harness.js)
# Supply Supabase session cookies or bearer so UI mode can authenticate.
HARNESS_COOKIE=
HARNESS_AUTHORIZATION=
# Optional: MCP bearer if the realtime session redacts headers.
HARNESS_MCP_TOKEN=
# Optional overrides; defaults are fine for most runs.
HARNESS_SESSION_URL=https://api.dexter.cash/realtime/sessions
HARNESS_TARGET_URL=https://beta.dexter.cash/
HARNESS_WAIT_MS=45000
HARNESS_PAGE_SIZE=5

# Logging controls
MCP_SCHEMA_DEBUG=0
# Optional comma-separated header names that, if present, label sessions in logs.
# Leave blank to disable (default).
MCP_SESSION_LABEL_HEADER=
